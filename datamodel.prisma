type User {
  id: ID! @unique
  name: String!
  birth: DateTime!
  email: String! @unique
  playlists: [Playlist!]!
}

type Playlist {
  id: ID! @unique
  name: String!
  owner: User!
  createdAt: DateTime!
  updatedAt: DateTime!
  songs: [Song!]!
}

type Song {
  id: ID! @unique
  source: String!
}

type JamSession {
  owner: User!
  playlist: Playlist
  users: [User!]!
}